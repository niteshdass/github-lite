A,WAAA,GAAA,EAAA;AACA,QAAA,WAAA,CAAA,QAAA,GAAA,KAAA,IAAA,EAAA;AACA,QAAA,WAAA,CAAA,gBAAA,GAAA,UAAA,IAAA,EAAA;AACA,QAAA,YAAA,CAAA,IAAA,CAAA,WAAA;AACA,eAAA,WAAA;AACA,OAXA;;AAYA,aAAA,YAAA;AACA,KAxBA;AAyBA,IAAA,UAzBA,wBAyBA;AAAA;;AACA,WAAA,OAAA,GAAA,IAAA;AACA,UAAA,IAAA,GAAA,KAAA,QAAA,IAAA,MAAA;AACA,WAAA,WAAA,GAAA,KAAA,QAAA;AACA,MAAA,IAAA,GAAA,IAAA,CAAA,KAAA,CAAA,GAAA,EAAA,IAAA,CAAA,EAAA,CAAA;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,2CAAA,IAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,GAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,MAAA,CAAA,WAAA,CAAA,GAAA,CAAA,IAAA,CAAA,KAAA,CAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,WAAA,EAAA,MAAA,CAAA,SAAA;AACA,OANA,EAOA,KAPA,CAOA,UAAA,KAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAVA;AAWA;AAzCA;AAjBA,CAAA","sourcesContent":["    <template>\n      <div class=\"wrapper\">\n        <v-card\n          color=\"grey lighten-4\"\n          flat\n          height=\"50px\"\n          tile\n        >\n          <v-toolbar id=\"scroll-target\" dense>\n            <router-link class=\"decoration__none\" to=\"/\"> <v-toolbar-title>GITHUB-LITE</v-toolbar-title></router-link>\n            <v-spacer></v-spacer>\n            <v-spacer></v-spacer>\n            <v-spacer></v-spacer>\n            <v-text-field\n              hide-details\n              label=\"Enter github account name\"\n              single-line\n              v-model=\"username\"\n              @keydown.enter=\"submitForm\"\n            ></v-text-field>\n            <v-btn @click=\"submitForm()\" icon>\n              <v-icon>mdi-magnify</v-icon>\n            </v-btn>\n          </v-toolbar>\n        </v-card>\n        <v-container v-if=\"loading\">\n          <LoadingBar />\n        </v-container>\n        <v-container v-if=\"!loading\">\n          <div class=\"search-result mx-auto\">\n            <v-card outlined flat class=\"bg-none py-4\">\n              <h2 v-if=\"dataItems.length\" class=\"text-center my-3\" >Github Users</h2>\n              <h2 v-else class=\"text-center my-3\" >Data not found ! please try again</h2>\n              <v-row class=\"ml-8\" >\n                <v-col cols=\"4\" md=\"4\" sm=\"12\" class=\"mx-auto\" v-for=\"item in dataItems\" :key=\"item.id\">\n                  <router-link :to=\"'/profile/' + item.userName\" class=\"decoration__none\">\n                    <v-card elevation=\"8\" outlined class=\"pa-3 my-3 search--item \">\n                      <v-row justify=\"center\" align=\"center\">\n                        <v-avatar size=\"200\" style=\"margin-top: 5%\">\n                          <img\n                          :src=\"item.userProfileImage\"\n                          alt=\"avater\"\n                          >\n                        </v-avatar>\n                      </v-row>\n                      <v-row justify=\"center\" style=\"margin-top: 20%\">\n                        <v-btn\n                        rounded\n                        color=\"info\"\n                        dark\n                        >\n                          {{ item.userName }}\n                          <v-icon\n                          dark\n                          right\n                          >\n                            mdi-arrow-right\n                          </v-icon>\n                        </v-btn>\n                      </v-row>\n                    </v-card>\n                  </router-link>\n                </v-col>\n              </v-row>\n            </v-card>\n          </div>\n          <v-btn\n          v-scroll=\"onScroll\"\n          v-show=\"fab\"\n          fab\n          dark\n          fixed\n          bottom\n          right\n          color=\"primary\"\n          @click=\"toTop\"\n          >\n            <v-icon>mdi-arrow-up</v-icon>\n          </v-btn>\n        </v-container>\n      </div>\n    </template>\n<script>\nimport axios from 'axios'\nimport _ from 'lodash'\nimport LoadingBar from './LoadingBar'\n\nexport default {\n  data: () => ({\n    username: '',\n    clicksubmit: '',\n    data: [],\n    dataItems: [],\n    loading: false,\n    fab: false\n  }),\n\n  components: {\n  // toolbar,\n    LoadingBar\n  },\n  async created () {\n    this.submitForm()\n  },\n  methods: {\n    onScroll (e) {\n      if (typeof window === 'undefined') return\n      const top = window.pageYOffset || e.target.scrollTop || 0\n      this.fab = top > 20\n    },\n    toTop () {\n      this.$vuetify.goTo(0)\n    },\n    prepareData (dataItems) {\n      const preparedData = []\n      _.map(dataItems, (dataItem) => {\n      /* eslint-disable camelcase */\n        const {\n          avatar_url = '',\n          login = ''\n        } = dataItem\n        const templateObj = {}\n        templateObj.userName = login || ''\n        templateObj.userProfileImage = avatar_url || ''\n        preparedData.push(templateObj)\n        return templateObj\n      })\n      return preparedData\n    },\n    submitForm () {\n      this.loading = true\n      let name = this.username || 'alex'\n      this.clicksubmit = this.username\n      name = name.split(' ').join('')\n      axios.get('https://api.github.com/search/users?q=' + name)\n        .then((res) => {\n          this.data = res.data\n          this.dataItems = this.prepareData(res.data.items)\n          this.loading = false\n          console.log('this.form', this.dataItems)\n        })\n        .catch((error) => {\n          this.loading = false\n          console.log(error)\n        })\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/components"}]}